{
  "name": "No mailto links",
  "description": "Disable all mailto href links",
  "version": "1",
  "manifest_version": 3,
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": ["clipboardWrite"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "runAt": "document_idle",
      "js": ["contentScript.js"]
    }
  ]
}
